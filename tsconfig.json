{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
        "baseUrl": ".",
    "paths": {
      "@features/*": ["src/features/*"],
      "@ui": ["src/ui"],
      "@ui/*": ["src/ui/*"],
      "@theme": ["src/theme"],
      "@theme/*": ["src/theme/*"],
      "@libs/*": ["src/libs/*"],
      "@hooks/*": ["src/hooks/*"],
      "@utils/*": ["src/utils/*"],
      "@stores/*": ["src/stores/*"],
      "@app/*": ["src/app/*"],
      "@env/*": ["src/env/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
